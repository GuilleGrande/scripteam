# ScripTeam Environment Configuration for bigapps.dev
# Copy to .env and fill in your values

# Domain Configuration
DOMAIN=scripteam.bigapps.dev
STAGING_DOMAIN=staging.scripteam.bigapps.dev
API_DOMAIN=api.scripteam.bigapps.dev
CDN_DOMAIN=cdn.scripteam.bigapps.dev

# Database Configuration
POSTGRES_PASSWORD=your_secure_password_here

# n8n Configuration
N8N_AUTH_USER=admin
N8N_AUTH_PASSWORD=your_n8n_password_here
N8N_DOMAIN=n8n.scripteam.bigapps.dev

# API Configuration
JWT_SECRET=your_jwt_secret_key_here
API_PORT=3000

# External AI Services
ELEVENLABS_API_KEY=your_elevenlabs_api_key
PLAYHT_API_KEY=your_playht_api_key

# Hostinger Configuration
HOSTINGER_CDN_URL=https://cdn.scripteam.bigapps.dev

# Monitoring & Error Tracking
SENTRY_DSN=your_sentry_dsn_here

# GitHub Container Registry (for CI/CD)
GITHUB_REPOSITORY=GuilleGrande/scripteam

# Development Settings
NODE_ENV=development
LOG_LEVEL=debug

# SSL/TLS (for production)
SSL_CERT_PATH=/etc/nginx/ssl/scripteam.bigapps.dev.crt
SSL_KEY_PATH=/etc/nginx/ssl/scripteam.bigapps.dev.key